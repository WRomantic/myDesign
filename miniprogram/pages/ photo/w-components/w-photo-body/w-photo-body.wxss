.photoBodyB {
  width: 100%;
  margin: 0 auto;
  height: 92vh;
  box-sizing: border-box;
  background-color: #e1e1e1;
  border-radius: 0.6rem;
  /* margin-top: 4vh; */
  text-align: center;
  border: 1px rgb(255, 255, 255) solid;
}

.photoBodyBH1 {
  font-size: 1.8rem;
  margin-top: 2rem;
  color: #338293;
}

.photoBodyB image {
  width: 80%;
  margin: 2rem 0 0.2rem 0;
}

.photoBodyBtn {
  width: 80%;
  height: 3rem;
  background-color: rgb(243, 243, 243);
  border-radius: 1.6rem;
  margin: 0 auto;
  margin-bottom: 1rem;
}
.photoBodyBtn button{
  margin-top: 0.4rem;
  background-color: rgb(243, 243, 243);
}
